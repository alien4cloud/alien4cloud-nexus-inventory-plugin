{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "alien4cloud-nexus-inventory-export-ui": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "projects/alien4cloud-nexus-inventory-export-ui",
      "prefix": "w4c",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/alien4cloud-nexus-inventory-export-ui",
            "index": "projects/alien4cloud-nexus-inventory-export-ui/index.html",
            "main": "projects/alien4cloud-nexus-inventory-export-ui/src/app/main.ts",
            "polyfills": "projects/alien4cloud-nexus-inventory-export-ui/src/app/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/alien4cloud-nexus-inventory-export-ui/favicon.ico",
              "projects/alien4cloud-nexus-inventory-export-ui/assets"
            ],
            "styles": [
              {
                "input": "projects/alien4cloud-nexus-inventory-export-ui/assets/styles/main.scss"
              },
              {
                "input": "projects/alien4cloud-nexus-inventory-export-ui/assets/styles/built/pink-bluegrey.css",
                "lazy": true,
                "bundleName": "pink-bluegrey"
              },
              {
                "input": "projects/alien4cloud-nexus-inventory-export-ui/assets/styles/built/deeppurple-amber.css",
                "lazy": true,
                "bundleName": "deeppurple-amber"
              },
              {
                "input": "projects/alien4cloud-nexus-inventory-export-ui/assets/styles/built/indigo-pink.css",
                "lazy": true,
                "bundleName": "indigo-pink"
              },
              {
                "input": "projects/alien4cloud-nexus-inventory-export-ui/assets/styles/built/purple-green.css",
                "lazy": true,
                "bundleName": "purple-green"
              }
            ],
            "scripts": [],
            "lazyModules": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "alien4cloud-nexus-inventory-export-ui:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "alien4cloud-nexus-inventory-export-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "alien4cloud-nexus-inventory-export-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/alien4cloud-nexus-inventory-export-ui/test.ts",
            "polyfills": "projects/alien4cloud-nexus-inventory-export-ui/src/app/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "projects/alien4cloud-nexus-inventory-export-ui/favicon.ico",
              "projects/alien4cloud-nexus-inventory-export-ui/assets"
            ],
            "styles": [
              {
                "input": "projects/alien4cloud-nexus-inventory-export-ui/assets/main.scss"
              },
              {
                "input": "projects/alien4cloud-nexus-inventory-export-ui/assets/pink-bluegrey.css",
                "lazy": true,
                "bundleName": "pink-bluegrey"
              },
              {
                "input": "projects/alien4cloud-nexus-inventory-export-ui/assets/deeppurple-amber.css",
                "lazy": true,
                "bundleName": "deeppurple-amber"
              },
              {
                "input": "projects/alien4cloud-nexus-inventory-export-ui/assets/indigo-pink.css",
                "lazy": true,
                "bundleName": "indigo-pink"
              },
              {
                "input": "projects/alien4cloud-nexus-inventory-export-ui/assets/purple-green.css",
                "lazy": true,
                "bundleName": "purple-green"
              }
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "alien4cloud-nexus-inventory-export-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "alien4cloud-nexus-inventory-export-ui:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "alien4cloud-nexus-inventory-export-ui"
}
